package rules

import pl.edu.pwsztar.universityrecruitment.UniversityCandidate;

// Kwalifikacja kobiet
rule "Women qualification (two_sets_of_rules_bonus_salience_modify_lock-on-active)"
dialect "java"
agenda-group "two_sets_of_rules_bonus_salience_modify_lock-on-active"
salience 200
lock-on-active true
when
    $uc : UniversityCandidate(gender == "Female");
then
	// System.out.println("Before WOMEN RULE examResult = " + $uc.getExamResult());
  	modify($uc) {
        setExamResult($uc.getExamResult() + 25),
    	appendLogger("Rule 'Women bonus' fired!")
    }
end